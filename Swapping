using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
namespace first_project
{
    class Program
    {
        static void Main(string[] args)
        {
            string name = "Ivanov", surname = "Ivan", tempname;

            Console.WriteLine("Hello, " + name + " " + surname);
            Console.WriteLine("Excuse me, wrong order");

            tempname = name;
            name = surname;
            surname = tempname;

            Console.WriteLine("Hello, " + name + " " + surname);
        }
    }
}
